<h3>Trades You Proposed</h3>
<% @from_notifications.each do |notification| %>
<div class="proposed-trade <%= notification.open ? "open" : "closed" %>">
	<div class="btn btn-inverse <%= notification.open ? "open" : "closed" %>-btn">
		<%= (notification.open ? "Open" : "Closed") %>
	</div><!--.respond-->
	<ul>
		<li>To: <%= link_to User.find(notification.to_user).username, User.find(notification.to_user) %></li>
		<li>Their Product: <%= link_to Product.find(notification.to_product).name, Product.find(notification.to_product) %></li>
		<li>Your Product: <%= link_to Product.find(notification.from_product).name, Product.find(notification.from_product) %></li>
	</ul>
</div>
<% end %>